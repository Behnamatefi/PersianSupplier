{
  "name": "Persian Supplier",
  "description": "Easily supply dynamic persian data",
  "author": "Hirad Arshadi",
  "authorEmail": "hiradwork@gmail.com",
  "compatibleVersion": 3,
  "homepage": "https://github.com/hiradarshadi/persiansupplier",
  "identifier": "com.box.persiansupplier",
  "bundleVersion": 1,
  "icon": "icon.png",
  "suppliesData": true,
  "commands": [
    {
      "script": "PersianSupplier.js",
      "handlers": {
        "actions": {
          "Startup": "onStartup",
          "Shutdown": "onShutdown",
          "SupplyNames": "onSupplyNames",
          "SupplyDates": "onSupplyDates",
          "SupplyCities": "onSupplyCities",
          "SupplyPhones": "onSupplyPhones"
        }
      }
    }
  ],
  "version": "1.0.0",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com/hiradarshadi/persiansupplier/master/.appcast.xml"
}